<div class= "container">
   <h3 class="text-center"> Contact form code!</h1> 
   
   <%= form_for "/contacts" do |f| %>
       <%= f label :name %>
       <%= f.text_field :name %>
       
        <%= f label :email %>
       <%= f.text_field :email %>
       
        <%= f label :comments %>
       <%= f.text_area :comments %>
       
       <%= f.submit "Submit" %>
   
   
   
   <form>
       <label>Name</label>
       <input type="text"/>
       <br/>
       <label>Email</label>
       <input type="text"/>
       <br/>
       <label>Comments</label>
       <input type="text"/>
       <br/>
       <input type="submit"/>
   </form>
</div>

